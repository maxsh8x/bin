{
  "extends": "airbnb",
  "globals": {
    "localStorage": true,
    "window": true,
    "require": true,
    "document": true
  },
  "parser": "babel-eslint",
  "rules": {
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": [
      "warn",
      {
        "aspects": [
          "invalidHref"
        ]
      }
    ]
  }
}